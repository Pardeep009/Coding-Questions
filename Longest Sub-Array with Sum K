#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t,n,k;
	cin>>t;
	while(t--)
	{
	    cin>>n>>k;
	    unordered_map<int,int> map;
	    int a[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }
	    int sum = 0;
	    int max = 0;
	    map[0] = -1;
	    for(int i=0;i<n;i++)
	    {
	        sum+=a[i];
	        if(map.find(sum-k)!=map.end())
	        {
	            max = max > (i-map[sum-k]) ? max : (i-map[sum-k]); 
	        }
	        if(map.find(sum)==map.end())
	            map[sum] = i;
	    }
	    cout<<max<<endl;
	}
	return 0;
}
